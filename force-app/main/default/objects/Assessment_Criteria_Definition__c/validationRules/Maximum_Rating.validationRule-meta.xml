<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Maximum_Rating</fullName>
    <active>true</active>
    <errorConditionFormula>Maximum_Rating__c &lt;= Minimum_Rating__c</errorConditionFormula>
    <errorDisplayField>Maximum_Rating__c</errorDisplayField>
    <errorMessage>Maximum Rating must be greater than Minimum Rating</errorMessage>
</ValidationRule>
