<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Range_Variable_Interest_Spread</fullName>
    <active>true</active>
    <errorConditionFormula>AND(!$Setup.Validation_Settings__c.Disable_Validation_Rules_on_Investment__c, Variable_Interest_Spread__c &gt;  Loan_Product__r.Maximum_Interest_Rate__c)</errorConditionFormula>
    <errorDisplayField>Variable_Interest_Spread__c</errorDisplayField>
    <errorMessage>Out of allowed range (Variable Interest Spread)</errorMessage>
</ValidationRule>
