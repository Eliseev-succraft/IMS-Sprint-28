<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Can_Be_Overridden_On_Loan_Percent</fullName>
    <active>true</active>
    <errorConditionFormula>AND(!$Setup.Validation_Settings__c.Disable_Validation_Rules_on_Loan_Product__c, TEXT(Setup_Fee__c) == &#39;% of Disbursement Amount&#39;, !CBO_Percent_Of_Disbursement_Amount__c, ISBLANK(Percent_Of_Disbursement_Amount__c))</errorConditionFormula>
    <errorDisplayField>Percent_Of_Disbursement_Amount__c</errorDisplayField>
    <errorMessage>Complete this field or check Can Be Overridden on Loan (% of Disbursement Amount)</errorMessage>
</ValidationRule>
