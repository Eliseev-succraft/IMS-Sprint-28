<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Maximum_Interest_Rate</fullName>
    <active>true</active>
    <errorConditionFormula>AND(!$Setup.Validation_Settings__c.Disable_Validation_Rules_on_Loan_Product__c, Maximum_Interest_Rate__c &lt; Minimum_Interest_Rate__c)</errorConditionFormula>
    <errorDisplayField>Maximum_Interest_Rate__c</errorDisplayField>
    <errorMessage>The maximum value must not be less than the minimum value (Maximum Interest Rate)</errorMessage>
</ValidationRule>
