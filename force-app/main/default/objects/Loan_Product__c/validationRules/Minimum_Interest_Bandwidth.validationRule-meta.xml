<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Minimum_Interest_Bandwidth</fullName>
    <active>true</active>
    <errorConditionFormula>AND(!$Setup.Validation_Settings__c.Disable_Validation_Rules_on_Loan_Product__c, Minimum_Interest_Bandwidth__c &gt; Maximum_Interest_Bandwidth__c)</errorConditionFormula>
    <errorDisplayField>Minimum_Interest_Bandwidth__c</errorDisplayField>
    <errorMessage>The minimum value must not be greater than the maximum value (Minimum Interest Bandwidth)</errorMessage>
</ValidationRule>
